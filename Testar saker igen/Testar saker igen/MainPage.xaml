<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Testar_saker_igen.MainPage">

    <Grid BackgroundColor="#F5F5F5" Padding="25 0">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <!-- #3E136C-->
        
        <StackLayout Grid.Row="1"  WidthRequest="50" HeightRequest="1000000" MinimumHeightRequest="10" Orientation="Vertical" >
            <Label 
                FontSize="Large"
                TextColor="#000" 
                Padding="20" 
                BackgroundColor="#FFC2D2" 
                FontAttributes="Bold"
                >Tid
            </Label>

            <!-- Denna stackLayout ska vara där alla tider är, alltså 0:00 till 23:00, men den ska även gå att scrolla-->
            <StackLayout Orientation="Vertical">
                
            </StackLayout>
            
        </StackLayout>


    </Grid>
    

</ContentPage>
